<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Intent.
    -->
    <changeSet id="20250902045313-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="response_id"
                                 baseTableName="intent"
                                 constraintName="fk_intent__response_id"
                                 referencedColumnNames="id"
                                 referencedTableName="intent_response"
                                 />

        <addForeignKeyConstraint baseColumnNames="bot_id"
                                 baseTableName="intent"
                                 constraintName="fk_intent__bot_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bot"
                                 />

        <addForeignKeyConstraint baseColumnNames="intent_id"
                                 baseTableName="rel_intent__entities"
                                 constraintName="fk_rel_intent__entities__intent_id"
                                 referencedColumnNames="id"
                                 referencedTableName="intent"
                                 />

        <addForeignKeyConstraint baseColumnNames="entities_id"
                                 baseTableName="rel_intent__entities"
                                 constraintName="fk_rel_intent__entities__entities_id"
                                 referencedColumnNames="id"
                                 referencedTableName="intent_entity"
                                 />
    </changeSet>
</databaseChangeLog>
